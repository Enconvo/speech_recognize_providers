{
  "$schema": "https://enconvo.com/schemas/extension.json",
  "name": "speech_recognize_providers",
  "version": "0.0.101",
  "description": "Speech-to-Text Providers",
  "title": "Speech-to-Text Providers",
  "icon": "icon.png",
  "author": "EnconvoAI",
  "categories": [
    "Provider"
  ],
  "license": "MIT",
  "type": "module",
  "commands": [
    {
      "name": "transcribe",
      "title": "Transcribe",
      "description": "Transcribe audio/video files",
      "icon": "transcribe.png",
      "mode": "no-view",
      "parameters": {
        "type": "object",
        "properties": {
          "context_files": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "The file path to be processed"
            },
            "description": "The files to be processed"
          },
          "audio_files": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "The file path to be processed, support audio / video file"
            },
            "description": "The audio files to be processed, support audio / video files"
          },
          "output_format": {
            "type": "string",
            "description": "The format for the transcription output",
            "enum": [
              "text",
              "txt"
            ],
            "default": "text"
          },
          "output_dir": {
            "type": "string",
            "description": "The output directory"
          }
        },
        "required": [
          "audio_files"
        ]
      },
      "preferences": [
        {
          "name": "output_format",
          "title": "Output Format",
          "description": "Choose the format for the transcription output",
          "type": "dropdown",
          "required": false,
          "data": [
            {
              "title": "Plain Text",
              "value": "text"
            },
            {
              "title": "TXT File",
              "value": "txt"
            }
          ],
          "default": "text"
        }
      ]
    },
    {
      "name": "enconvo",
      "title": "Enconvo Cloud Plan",
      "description": "Enconvo Speech-to-Text Provider that allows you to use high speed online Speech-to-Text service",
      "icon": "enconvo.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "speechRecognitionLanguage",
          "title": "Language",
          "description": "The Language .",
          "type": "dropdown",
          "required": false,
          "dataProxy": "speech_recognize_providers|azure_voices",
          "default": "en-US"
        }
      ]
    },
    {
      "name": "azure",
      "title": "Microsoft",
      "description": "Microsoft Speech-to-Text Provider that allows you to use Microsoft Azure's online Speech-to-Text service",
      "icon": "azure.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "speechRecognitionLanguage",
          "title": "Language",
          "description": "The Language .",
          "type": "dropdown",
          "required": false,
          "dataProxy": "speech_recognize_providers|azure_voices",
          "default": "en-US"
        },
        {
          "name": "region",
          "title": "Resource Region",
          "description": "The region of the resource.",
          "type": "dropdown",
          "required": true,
          "default": "eastus",
          "defaultProxy": "KEY_AZURE_SPEECH_REGION",
          "dataProxy": "speech_recognize_providers|azure_speech_regions"
        },
        {
          "name": "resource_key",
          "title": "Resource Key",
          "description": "How to get api key? [ðŸ”‘here](https://learn.microsoft.com/en-us/azure/ai-services/speech-service/overview)",
          "type": "password",
          "required": true,
          "default": "",
          "defaultProxy": "KEY_AZURE_SPEECH",
          "placeholder": "********"
        }
      ]
    },
    {
      "name": "assembly_ai",
      "title": "Assembly AI",
      "description": "Assembly AI Speech-to-Text Provider that allows you to use Assembly AI's online Speech-to-Text service",
      "icon": "assemblyai.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "speechRecognitionLanguage",
          "title": "Language",
          "description": "The Language .",
          "type": "dropdown",
          "required": false,
          "dataProxy": "speech_recognize_providers|assemblyai_languages",
          "default": "auto"
        },
        {
          "name": "api_key",
          "title": "API Key",
          "description": "API Key , [ðŸ”‘here](https://www.assemblyai.com/app/)",
          "type": "password",
          "required": true,
          "default": "",
          "defaultProxy": "KEY_ASSEMBLY_AI_APIKEY",
          "placeholder": "********"
        }
      ]
    },
    {
      "name": "youdao",
      "title": "Youdao",
      "description": "Youdao Speech-to-Text Provider that allows you to use Youdao's online Speech-to-Text service",
      "icon": "youdao.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "speechRecognitionLanguage",
          "title": "Language",
          "description": "The Language .",
          "type": "dropdown",
          "required": false,
          "dataProxy": "speech_recognize_providers|youdao_languages",
          "default": "zh-CHS"
        },
        {
          "name": "youdao_api_key",
          "title": "API Key",
          "description": "youdao_api_key",
          "type": "password",
          "required": true,
          "default": "",
          "defaultProxy": "KEY_YOUDAO_SPEECH_APIKEY",
          "placeholder": "********"
        },
        {
          "name": "youdao_api_secret",
          "title": "API Secret",
          "description": "youdao_api_secret",
          "type": "password",
          "required": true,
          "default": "",
          "defaultProxy": "KEY_YOUDAO_SPEECH_API_SECRET",
          "placeholder": "********"
        }
      ]
    },
    {
      "name": "deepgram",
      "title": "Deepgram",
      "description": "Deepgram Speech-to-Text Provider that allows you to use Deepgram's online Speech-to-Text service",
      "icon": "deepgram.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "speechRecognitionLanguage",
          "title": "Language",
          "description": "The Language .",
          "type": "dropdown",
          "required": false,
          "dataProxy": "speech_recognize_providers|deepgram_languages",
          "default": "en"
        },
        {
          "name": "deepgram_token",
          "title": "Deepgram Token",
          "description": "Deepgram Token",
          "type": "password",
          "required": true,
          "default": "",
          "defaultProxy": "KEY_DEEPGRAM_TOKEN",
          "placeholder": "********"
        },
        {
          "name": "modelName",
          "title": "ModelName",
          "description": "Speech-to-text Model [more...](https://developers.deepgram.com/docs/models-languages-overview#nova-2)",
          "type": "dropdown",
          "required": false,
          "data": [
            {
              "title": "Nova-2",
              "value": "nova-2"
            },
            {
              "title": "Nova",
              "value": "nova"
            },
            {
              "title": "Enhanced",
              "value": "enhanced"
            },
            {
              "title": "Base",
              "value": "base"
            }
          ],
          "default": "nova-2"
        }
      ]
    },
    {
      "name": "groq",
      "title": "Groq Whisper (Beta)",
      "description": "Groq Speech-to-Text Provider that allows you to use [Groq](https://console.groq.com/docs/speech-text)'s online Speech-to-Text service",
      "icon": "groq.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "speechRecognitionLanguage",
          "title": "Language",
          "description": "Specify the language for transcription. We highly recommend setting the language if you know it as specifying a language may improve transcription accuracy and speed.",
          "type": "dropdown",
          "required": false,
          "default": "en",
          "dataProxy": "translate|languages"
        },
        {
          "name": "apiKey",
          "title": "Groq API Key",
          "description": "Groq API Key",
          "type": "password",
          "required": true,
          "default": "",
          "defaultProxy": "KEY_GROQ_APIKEY",
          "placeholder": "gsk-********"
        },
        {
          "name": "prompt",
          "title": "Prompt",
          "description": "Provide context or specify how to spell unfamiliar words (limited to 224 tokens).",
          "type": "textfield",
          "default": ""
        },
        {
          "name": "modelName",
          "title": "ModelName",
          "description": "Speech-to-text Model [more...](https://docs.groq.com/docs/speech-to-text/models)",
          "type": "dropdown",
          "required": false,
          "data": [
            {
              "title": "Whisper Large V3 Turbo",
              "value": "whisper-large-v3-turbo"
            },
            {
              "title": "whisper-large-v3",
              "value": "Whisper large-v3"
            },
            {
              "title": "Distil-Whisper English",
              "value": "distil-whisper-large-v3-en"
            }
          ],
          "default": "whisper-large-v3-turbo"
        }
      ]
    },
    {
      "name": "whisperOffline",
      "title": "Local Whisper (Beta)",
      "description": "Local Whisper Speech-to-Text Provider that runs on your local machine",
      "icon": "local_whisper.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "localWhisper",
          "title": "Local Whisper Settings",
          "type": "localWhisper"
        }
      ]
    },
    {
      "name": "azure_voices",
      "title": "azure_voices",
      "description": "get azure_voices list",
      "icon": "icon.png",
      "commandType": "function_command",
      "showInCommandList": false,
      "mode": "no-view"
    },
    {
      "name": "azure_speech_regions",
      "title": "azure_speech_regions",
      "description": "get azure_speech_regions list",
      "icon": "icon.png",
      "commandType": "function_command",
      "showInCommandList": false,
      "mode": "no-view"
    },
    {
      "name": "youdao_languages",
      "title": "youdao_languages",
      "description": "get youdao_languages list",
      "icon": "icon.png",
      "commandType": "function_command",
      "showInCommandList": false,
      "mode": "no-view"
    },
    {
      "name": "deepgram_languages",
      "title": "deepgram_languages",
      "description": "get deepgram_languages list",
      "icon": "icon.png",
      "commandType": "function_command",
      "showInCommandList": false,
      "mode": "no-view"
    },
    {
      "name": "assemblyai_languages",
      "title": "assemblyai_languages",
      "description": "get assemblyai_languages list",
      "icon": "icon.png",
      "commandType": "function_command",
      "showInCommandList": false,
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@deepgram/sdk": "^3.9.0",
    "@enconvo/api": "link:/Users/ysnows/Documents/Project/enconvo.nodejs/enconvo_api",
    "assemblyai": "^4.8.0",
    "axios": "^1.7.9",
    "form-data": "^4.0.1",
    "fs-extra": "^11.3.0",
    "groq-sdk": "^0.12.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.42.0",
    "music-metadata": "^10.7.0",
    "openai": "^4.77.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --list-different \"**/*.ts\"",
    "build": "enconvo",
    "dev": "enconvo --dev"
  },
  "minAppVersion": "1.8.8"
}
