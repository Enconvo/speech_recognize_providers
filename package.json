{
  "$schema": "https://enconvo.com/schemas/extension.json",
  "name": "speech_recognize_providers",
  "version": "0.0.3",
  "description": "Speech To Text Providers",
  "title": "Speech To Text Providers",
  "icon": "icon.png",
  "author": "EnconvoAI",
  "categories": [
    "Speech To Text"
  ],
  "license": "MIT",
  "type": "module",
  "commands": [
    {
      "name": "azure",
      "title": "Microsoft Speech To Text",
      "description": "Microsoft Speech To Text Provider that allows you to use Microsoft Azure's online text-to-speech service",
      "icon": "azure.png",
      "mode": "llm",
      "preferences": [
        {
          "name": "speechRecognitionLanguage",
          "title": "Language",
          "description": "The Language .",
          "type": "dropdown",
          "required": false,
          "data": [
            {
              "value": "en-US",
              "title": "en-US"
            }
          ],
          "dataProxy": "speech_recognize_providers|azure_voices",
          "default": "en-US"
        },
        {
          "name": "region",
          "title": "Resource Region",
          "description": "The region of the resource.",
          "type": "textfield",
          "required": true,
          "default": ""
        },
        {
          "name": "resource_key",
          "title": "Resource Key",
          "description": "The resource key of the resource.",
          "type": "password",
          "required": true,
          "default": ""
        }
      ]
    },
    {
      "name": "youdao",
      "title": "Youdao Speech To Text",
      "description": "Youdao Speech To Text Provider that allows you to use Youdao's online text-to-speech service",
      "icon": "youdao.png",
      "mode": "llm",
      "preferences": [
        {
          "name": "speechRecognitionLanguage",
          "title": "Language",
          "description": "The Language .",
          "type": "dropdown",
          "required": false,
          "data": [
            {
              "value": "zh-CHS",
              "title": "中文（简体，中国）"
            }
          ],
          "dataProxy": "speech_recognize_providers|youdao_languages",
          "default": "zh-CHS"
        },
        {
          "name": "youdao_api_key",
          "title": "API Key",
          "description": "youdao_api_key",
          "type": "password",
          "required": true,
          "default": ""
        },
        {
          "name": "youdao_api_secret",
          "title": "API Secret",
          "description": "youdao_api_secret",
          "type": "password",
          "required": true,
          "default": ""
        }
      ]
    },
    {
      "name": "deepgram",
      "title": "deepgram Speech To Text",
      "description": "deepgram Speech To Text Provider that allows you to use DeppGram's online text-to-speech service",
      "icon": "deepgram.png",
      "mode": "llm",
      "preferences": [
        {
          "name": "speechRecognitionLanguage",
          "title": "Language",
          "description": "The Language .",
          "type": "dropdown",
          "required": false,
          "data": [
            {
              "value": "en",
              "title": "English"
            }
          ],
          "dataProxy": "speech_recognize_providers|deepgram_languages",
          "default": "en"
        },
        {
          "name": "deepgram_token",
          "title": "deepgram_token",
          "description": "deepgram_token",
          "type": "password",
          "required": true,
          "default": ""
        }
      ]
    },
    {
      "name": "azure_voices",
      "title": "azure_voices",
      "description": "get azure_voices list",
      "icon": "icon.png",
      "showInCommandList": false,
      "mode": "no-view"
    },
    {
      "name": "youdao_languages",
      "title": "youdao_languages",
      "description": "get youdao_languages list",
      "icon": "icon.png",
      "showInCommandList": false,
      "mode": "no-view"
    },
    {
      "name": "deepgram_languages",
      "title": "deepgram_languages",
      "description": "get deepgram_languages list",
      "icon": "icon.png",
      "showInCommandList": false,
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@enconvo/api": "^0.1.125",
    "axios": "^1.4.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.36.0",
    "msedge-Speech To Text": "^1.3.4",
    "openai": "^4.33.0"
  },
  "devDependencies": {
    "@types/node": "^18.17.14",
    "eslint": "^8.33.0",
    "prettier": "^2.8.3",
    "tsup": "^7.2.0",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --list-different \"**/*.ts\"",
    "build": "enconvo",
    "dev": "enconvo --dev"
  },
  "minAppVersion": "1.8.8"
}
